{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "babel ./src --out-dir build",
    "start": "node ./build/bin/www",
    "dev": "SET DEBUG=backend:* & cross-env NODE_ENV=development nodemon --inspect --ignore __tests__ --exec babel-node ./src/bin/www",
